                           def list = [];
                            for (int j=0; j<10; j++) {
                                def k=i<10?"0"+i:i;
                                list.add("_"+k+"_"+j);
                            }
                             map.put(i,list);
                        };
                        return map; 
                ]]>
        </tbPrefix>
    </tableRule>
